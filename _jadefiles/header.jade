mixin nav-content()
  ul.hide-ul
    li: a.youtube(href="https://www.youtube.com/channel/UCVzMry2PfkaisRTrgLef4jw") <i class="fa fa-youtube-play fa-lg"></i>
    li: a.facebook(href="https://www.facebook.com/reversengineers/") <i class="fa fa-facebook-square fa-lg"></i>
    li: a.instagram(href="https://www.instagram.com/reverse.engineers/") <i class="fa fa-instagram fa-lg"></i>
    li: a.snapchat(href="#snapchat") <i class="fa fa-snapchat-square fa-lg"></i>
    li: a.mail(href="#mail") <i class="fa fa-envelope-square fa-lg"></i>    

.mobile-nav
  +nav-content()

header
  .header-position.active
    a.logo(href="/")
    
    nav.site-nav
      +nav-content()
    
    .mobile-nav-toggle: span
